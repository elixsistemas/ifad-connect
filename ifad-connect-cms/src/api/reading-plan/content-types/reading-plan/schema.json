{
  "kind": "collectionType",
  "collectionName": "reading_plans",
  "info": {
    "singularName": "reading-plan",
    "pluralName": "reading-plans",
    "displayName": "Reading Plan"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "description": {
      "type": "blocks"
    },
    "durationDays": {
      "type": "integer"
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "integer"
    },
    "devotionals": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::devotional.devotional"
    }
  }
}
